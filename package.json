    "haversine": "^1.1.1",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@wdio/cli": "^6.1.15",
    "@wdio/local-runner": "^6.1.14",
    "@wdio/mocha-framework": "^6.1.14",
    "@wdio/selenium-standalone-service": "^6.1.14",
    "@wdio/spec-reporter": "^6.1.14",
    "@wdio/sync": "^6.1.14",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2"
  },
  "standard": {
    "globals": [
      "afterEach",
