  <h2>This is an application to display users who are living in London, and users who are currently within 50 miles of London.</h2>

  <div id="residents">
    <%- include('_cards', { cards: residents }) %>
  </div>

  <div id="nearby">
    <%- include('_cards', { cards: nearby }) %>
  </div>

</body>
</html>